[deploy_commercial:children]
deploy_commercial_ordering

[dev]
deploy_commercial_ordering_dev

[uat]
deploy_commercial_ordering_uat

[prod]
deploy_commercial_ordering_prod

[commercial:children]
deploy_commercial_ordering

[ordering:children]
deploy_commercial_ordering

[deploy:children]
deploy_commercial

